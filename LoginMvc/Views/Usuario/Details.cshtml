@using LoginMvc.Models
@model Usuario
@{
    ViewData["Title"] = "Details";
}

<h2>@ViewData["Title"]</h2>

<h4>Usuario</h4>
<hr />
<div class="row">
    <div class="col-md-4">

            
            <div class="form-group">
                <p>@Html.DisplayNameFor(usuario => usuario.Name)</p>
                <p>@Html.DisplayFor(usuario => usuario.Name)</p>
            </div>
            <div class="form-group">
                <p>@Html.DisplayNameFor(usuario => usuario.Email)</p>
                <p>@Html.DisplayFor(usuario => usuario.Email)</p>
            </div>
            <div class="form-group">
                <p>@Html.DisplayNameFor(usuario => usuario.Password)</p>
                <p>@Html.DisplayFor(usuario => usuario.Password)</p>
            </div>
           
          
    </div>
</div>
